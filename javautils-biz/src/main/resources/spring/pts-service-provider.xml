<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">
 	
 	<bean class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
        <property name="systemPropertiesModeName" value="SYSTEM_PROPERTIES_MODE_OVERRIDE" />
    </bean>
 	
    <!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:application name="wt_pts"/>
 
 	<dubbo:registry address="zookeeper://localhost:2181"/>
 	
 	<dubbo:provider threads="200" delay="-1"  />
 	
    <!-- 用dubbo协议在20880端口暴露服务 -->
    <dubbo:protocol name="dubbo" port="20898" threads="200" />
    
 	<dubbo:monitor protocol="registry"/>

  <!--  <dubbo:service interface="com.wt.pts.service.DoubleAddYeildService" ref="doubleAddYeildServiceImpl" version="${pts.dubbo.version}" />
    <bean id="doubleAddYeildServiceImpl" class="com.wt.pts.biz.export.DoubleAddYeildServiceImpl" />-->
</beans>